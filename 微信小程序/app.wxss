/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: space-between; */
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 
page{
  height: 100%;
}
.topBar{
  width: 100%;
  height: 120rpx;
  background-color: rgb(253, 172, 190);
  position:fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index:99;
}
.logoContainer{
  padding: 0rpx;
  overflow: hidden;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-top: 5rpx;
  border:3px solid white;
  align-items: center;
  text-align: center;
  color :white;
  background: #fff;
}
.logoImage{
  height:100%;
  width:100%;
}
.userinfo {
  position: fixed;
  display: flex;
  align-items: center;
  top:20rpx;
  left:10rpx;
  color: rgb(255, 252, 253);
}
.userinfo-avatar {
  padding: 0rpx;
  overflow: hidden;
  width: 75rpx;
  height: 75rpx;
  margin: 5rpx;
  border-radius: 50%;
}

